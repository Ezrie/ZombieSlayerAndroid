<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    tools:context="com.zombie.menu.Views.Characters">

    <Button
        android:id="@+id/btnBack"
        android:layout_width="120dp"
        android:layout_height="55dp"
        android:background="@android:color/holo_green_dark"
        android:text="@string/back"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        />
    <Button
        android:id="@+id/characterOne"
        android:layout_width="120dp"
        android:layout_height="55dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="90dp"
        android:background="@android:color/holo_green_dark"
        android:text="charOne"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        />
    <Button
        android:id="@+id/characterTwo"
        android:layout_width="120dp"
        android:layout_height="55dp"
        android:layout_below="@id/characterOne"
        android:layout_alignStart="@id/characterOne"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_green_dark"
        android:text="charaTwo"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        />

</RelativeLayout>
